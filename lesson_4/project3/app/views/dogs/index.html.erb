<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>
 
  <% @dog.each do |dog| %>
    <tr>
      <td><%= dog.title %></td>
      <td><%= dog.text %></td>
      <td><%= link_to 'Show', dog_path(dog) %>
      <td><%= link_to 'Edit', edit_dog_path(dog) %>
      <td><%= link_to 'Destroy', dog_path(dog),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
      </td>
    </tr>
  <% end %>
</table>